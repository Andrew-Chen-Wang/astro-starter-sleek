{
	"name": "astro-starter",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "npm run i18n:generate && astro dev",
		"build": "npm run pwa:generate-assets && npm run i18n:generate && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"script:change-language-in-dynamic-pages": "node ./src/scripts/change-language-in-dynamic-pages.js",
		"i18n:generate": "astro-i18next generate && npm run script:change-language-in-dynamic-pages",
		"pwa:generate-assets": "pwa-assets-generator --preset minimal public/logo.svg"
	},
	"dependencies": {
		"@astrojs/alpinejs": "^0.3.0",
		"@astrojs/mdx": "^1.0.3",
		"@astrojs/rss": "^3.0.0",
		"@astrojs/sitemap": "^3.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@fontsource-variable/inter": "^5.0.8",
		"@types/alpinejs": "^3.7.2",
		"alpinejs": "^3.13.0",
		"astro": "^3.0.12",
		"astro-netlify-cms": "^0.5.4",
		"tailwindcss": "^3.3.3"
	},
	"devDependencies": {
		"@vite-pwa/assets-generator": "^0.0.10",
		"astro-i18next": "^1.0.0-beta.21",
		"astro-icon": "^0.8.1",
		"astro-seo": "^0.8.0",
		"prettier": "^3.0.3",
		"prettier-plugin-astro": "^0.12.0"
	}
}
